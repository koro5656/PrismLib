<?xml version="1.0" encoding="utf-8"?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            x:Class="PrismLib.Views.CompositeCommandTabbedPage"
            Title="{Binding Title}">
    <TabbedPage.ToolbarItems>
        <ToolbarItem Text="SaveAll"
                     Command="{Binding ApplicationCommands.SaveCommand2}"/>
        <ToolbarItem Text="Click"
                     Command="{Binding ApplicationCommands.ActionCommand}"/>
    </TabbedPage.ToolbarItems>
</TabbedPage>
